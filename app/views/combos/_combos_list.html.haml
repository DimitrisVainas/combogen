=# render 'shared/flash_messages'
.list.card
  - unless @combos.empty?
    - @combos.each do |combo|
      = render partial: 'combos/combo_row', locals: {combo: combo}
  - else
    .list-row
      %h3= empty_message
.pagination-container
  = will_paginate @combos, renderer: BootstrapPagination::Rails